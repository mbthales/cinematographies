{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"paths": {
			"prisma/*": ["./prisma/*"],
			"controllers/*": ["./src/controllers/*"],
			"helpers/*": ["./src/helpers/*"],
			"lib/*": ["./src/lib/*"],
			"middlewares/*": ["./src/middlewares/*"],
			"repositories/*": ["./src/repositories/*"],
			"routes/*": ["./src/routes/*"],
			"types/*": ["./src/types/*"],
			"validators/*": ["./src/validators/*"]
		}
	}
}
